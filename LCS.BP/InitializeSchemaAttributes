      SUBROUTINE InitializeSchemaAttributes(json_file_path)
      *$INCLUDE QMH.LIBINC
      OPEN "SCHEMA_ATTRIBUTES" TO SCHEMA_ATTR_FILE ELSE STOP "Cannot open SCHEMA_ATTRIBUTES file"

      * Read JSON file
      OPEN "json_file_path" TO FILE_PTR ELSE
         CRT "Cannot open file: ": json_file_path
         RETURN
      END

      READ json_data FROM FILE_PTR

      * Parse JSON and store in SCHEMA_ATTRIBUTES file
      json_lines = DCOUNT(json_data, @VM)
      FOR i = 1 TO json_lines
         schema_line = json_data<i>
         schema_item = PARSE(schema_line, ":", 1)
         schema_attributes = PARSE(schema_line, ":", 2)
         WRITE schema_attributes ON SCHEMA_ATTR_FILE, schema_item
      NEXT i

      CRT "Schema attributes initialized."
      END

