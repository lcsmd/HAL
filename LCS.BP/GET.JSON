   OPEN 'LCS.TXT' TO F.LCS ELSE PRINT 'LCS.TXT';STOP
   READ CONV.DATA FROM F.LCS, "CONVERSATION.JSON" ELSE CONV.DATA = ""
   OPEN 'CONVERSATION' TO F.COLLECTION ELSE PRINT 'COLLECTION';STOP
   JSON.DOCUMENT = JPARSE(CONV.DATA)
   CRT JBUILD(JSON.DOCUMENT)
   SCAN JSON.DOCUMENT RESET
   LOOP
      SCAN JSON.DOCUMENT SETTING NAME, VALUE ELSE EXIT
      DISPLAY 'Element ':NAME : ' has value ' : VALUE
   REPEAT
   WRITE JSON.DOCUMENT TO F.COLLECTION, "1"
END
