      PROGRAM LoadNextIds
      INCLUDE QM.INC

      OPEN "NEXTIDS" TO NEXTIDS.FILE ELSE
         PRINT "Unable to open NEXTIDS file"
         STOP
      END

      FOR i = 1 TO DCOUNT(NEXT_IDS, @AM)
         READ NEXT_IDS(i) FROM NEXTIDS.FILE, STR(i, 2, "0") ELSE NEXT_IDS(i) = 100000000
      NEXT i

      PRINT "Next record IDs loaded successfully"
      END PROGRAM

