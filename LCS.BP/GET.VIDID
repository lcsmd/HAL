      function get.vidid(yturlinput)
      YTURL = "https://youtu.be/"
      YTURL2="https://www.youtube.com/watch?v="
      UYTURL = UPCASE(YTURL)
      UYTURL2 = UPCASE(YTURL2)
      yturlinput=CHANGE(yturlinput,YTURL,"")
      yturlinput=CHANGE(yturlinput,UYTURL,"")
      yturlinput=CHANGE(yturlinput,YTURL2,"")
      yturlinput=CHANGE(yturlinput,UYTURL2,"")
      yturlinput=TRIM(yturlinput)
      return(yturlinput)
      END

