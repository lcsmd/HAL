      OPEN 'PAYEES' TO PAYEES ELSE PRINT 'PAYEES';STOP
      PRINT "KEY:":;INPUT KEY
      IF KEY = "" THEN KEY = "BANK"
      SELECT.STR = "SELECT PAYEES WITH NAME LIKE ...'":KEY:"'..."
      XMISC = "TRANSACTION ACCOUNT SELECTOR"
      XMISC<5> = "ACCT.ID ACCOUNT.NAME"
      XMISC<7> = 5
      EXECUTE SELECT.STR CAPTURING CAP
      READLIST KEYS THEN
         CALL SELECTOR(KEYS,XMISC)
         PRINT XMISC
      END

