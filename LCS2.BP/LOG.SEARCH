     SUBROUTINE LOG.SEARCH(INARGS)

     EQU TYPE TO INARGS<1>
     EQU SEARCH TO INARGS<2>
     EQU FILE.KEY TO INARGS<3>
     EQU RESULT.COUNT TO INARGS<4>
     OPEN 'SEARCH.LOG' TO SEARCH.LOG ELSE PRINT 'SEARCH.LOG';INPUT ANS,1
     EXECUTE "WHO" CAPTURING WHO.LINE
     USER = TRIM(FIELD(WHO.LINE<1>,"\",2))
     TODAY = DATE()
     NOW = TIME()
     TIMESTAMP = TODAY:"*":NOW
     KEY = USER:"|":TIMESTAMP
     WRITE INARGS ON SEARCH.LOG,KEY
     RETURN



