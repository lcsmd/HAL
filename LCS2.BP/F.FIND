     FUNCTION F.FIND(FILE.NAME,LIST,EXPRESSION)
     LM =1
     EXPRESSION = CHANGE(EXPRESSION," E)","")
     EXPRESSION = CHANGE(EXPRESSION,"TRAP","")
     SELECT.STR = "SELECT ":FILE.NAME:" WITH ":EXPRESSION
     IF LM THEN PRINT SELECT.STR
     EXECUTE SELECT.STR CAPTURING CAP
     IF LM THEN
          PRINT CAP<1>
          PRINT CAP<2>
          PRINT CAP<3>
     END
     READLIST KEYS ELSE KEYS = ""
     RETURN (KEYS)

