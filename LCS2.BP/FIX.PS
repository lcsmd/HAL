      OPEN 'PRODUCT' TO PRODUCT ELSE PRINT 'PRODUCT' ; STOP
      OPEN 'UPC.PRODUCT.XREF' TO UPC.PRODUCT.XREF ELSE PRINT 'UPC.PRPODUCT.XREF' ; STOP
      PROMPT ''
      CT=0
      SELECT.STR = "SELECT PRODUCT WITH UPC.PS <> PS_NUM"
      PRINT SELECT.STR 
      EXECUTE SELECT.STR CAPTURING CAP
      PRINT CAP
      PRINT 'C TO CONTINUE OR Q TO QUIT'
      INPUT ANS,1
      IF ANS = 'Q' THEN STOP
      IF ANS <> 'C' THEN STOP
      LOOP
         CT+=1
      WHILE READNEXT AA DO
         READV UPC FROM PRODUCT,AA,18 THEN
            READV PS.NUM FROM UPC.PRODUCT.XREF,UPC,1 ELSE
               PS.NUM = ""
            END
            WRITEV PS.NUM ON PRODUCT,AA,61
         END
         PRINT AA:" WRITTEN WITH PS.NUM ":PS.NUM
      REPEAT
      PRINT 'FINISHED'
   END
