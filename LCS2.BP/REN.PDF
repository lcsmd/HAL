
      OPEN 'EXHIBITS' TO F.EXHIBITS ELSE PRINT 'EXHIBITS';STOP
      EXECUTE 'SSELECT EXHIBITS'

      LOOP
         READNEXT EXH.ID THEN
            READ EXH.B FROM F.EXHIBITS,EXH.ID THEN
               IF EXH.B<10> THEN
                  CMD = "SH REN E:\EXHIBITS\":EXH.B<10>:" ":EXH.ID:".PDF"
                  EXECUTE CMD
                  PRINT CMD
               END
            END
         END ELSE EXIT
      REPEAT
      END
