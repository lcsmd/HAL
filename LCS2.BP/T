     OPEN 'LCS.CONTROL' TO LCS.CONTROL ELSE PRINT 'LCS.CONTROL';STOP
     READ SCREEN FROM LCS.CONTROL,"SCREEN" THEN
          B=SCREEN
          GOSUB SHOW.SCREEN
     END
     READ SCREEN2 FROM LCS.CONTROL,"SCREEN2" THEN
          B = SCREEN2
          GOSUB SHOW.SCREEN:
     END
     STOP
SHOW.SCREEN:
     FOR X = 1 TO 500
          A = B[X,1]
          PRINT "(":X:"-":SEQ(A):")  ":A
     NEXT X

     RETURN
