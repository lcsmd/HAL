     SUBROUTINE GET.CUSTOMER.REC(INARGS,OUTARGS)
     OPEN 'CUSTOMER' TO CUSTOMER ELSE PRINT 'CUSTOMER';STOP
     CKEY=INARGS<1>
     READ BUFF FROM CUSTOMER,CKEY ELSE
          SELECT.STR = "SELECT CUSTOMER WITH CUSTOMER.INDEX = ":CKEY
          EXECUTE SELECT.STR CAPTURING CAP
          READLIST CKEYS THEN
               OUTARGS = CKEYS
          END ELSE OUTARGS = ""
     END
     RETURN
