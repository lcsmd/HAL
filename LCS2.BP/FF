      OPEN 'PEXX' TO PEXX ELSE PRINT 'PEXX'; STOP
      OPEN 'FACTS' TO FACTS ELSE PRINT 'FACTS'; STOP
      OPEN 'EXHIBITS' TO EXHIBITS ELSE STOP
      OPEN 'POT.EXH' TO POT.EXH ELSE STOP
      SSELECT EXHIBITS
      CT = 0
      LOOP
         CT+=1
         PRINT CT
         READNEXT EXH.ID THEN
            PRINT EXH.ID
            READ EXH.B FROM EXHIBITS,EXH.ID THEN
               FAC.ID = EXH.B<9>
               READ FAC.B FROM FACTS,FAC.ID THEN
                  PEX.ID = EXH.B<10>:".PDF"
                  READ PEX.B FROM POT.EXH,PEX.ID THEN
                     EXH.B<1> = PEX.B<1>
                     EXH.B<2> = PEX.B<2>
                     EXH.B<5> = PEX.B<5>
                     EXH.B<10> = PEX.ID
                     FAC.B<9> = PEX.ID[1,5]
                     WRITE EXH.B ON EXHIBITS,EXH.ID
                     WRITE FAC.B ON FACTS,FAC.ID
                  END
               END
            END
         END ELSE EXIT
      REPEAT
      END
