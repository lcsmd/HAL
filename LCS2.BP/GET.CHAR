      PROGRAM GET.CHAR
      P=@SENTENCE
      FN=FIELD(P," ",2)
      IN=FIELD(P," ",3)
      ATT=FIELD(P," ",4)
      CH=FIELD(P," ",5)
      * GET.CHAR FILENAME ITEMNAME ATTRIBUTENUM CHARNUM
      OPEN FN TO F.FN ELSE PRINT FN ; STOP
      READV BUFF FROM F.FN,IN,ATT THEN
         IF CH THEN CHARACTER=BUFF[CH,1]
      END ELSE PRINT "item not found" ; STOP
      CHARACTER=BUFF
      PRINT CHARACTER
      PRINT ASCII(CHARACTER)
      PRINT SEQ(CHARACTER)
   END
