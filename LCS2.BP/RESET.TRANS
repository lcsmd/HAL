     OPEN 'TRANS' TO TRANS ELSE PRINT 'TRANS';STOP
     INCLUDE FILE.EQU TRANS.EQ
     SELECT TRANS
     LOOP WHILE READNEXT TR.ID DO
          PRINT TR.ID
          READ TR.B FROM TRANS,TR.ID THEN
               TR.CATEGORY = ""
               TR.PAYEE = ""
               TR.CLASS = ""
               TR.DEPT = ""
               TR.RULE = ""
               WRITE TR.B ON TRANS,TR.ID
          END
     REPEAT
