     OPEN 'I2.PRODUCT' TO I2.PRODUCT ELSE PRINT 'I2.PRODUCT';STOP
     OPEN 'COMM' TO COMM ELSE PRINT 'COMM';STOP

     EQU MFR.PIK TO I2BUFF<64>
     EQU COMM.PIK TO I2BUFF<33>

     SELECT I2.PRODUCT
     CT=0
     LOOP
          CT+=1
     WHILE READNEXT PS.NUM DO
          READ I2BUFF FROM I2.PRODUCT,PS.NUM THEN
               IF LEN(MFR.PIK)+LEN(COMM.PIK) <> 8 THEN
                    MFR.PIK = ("0000":MFR.PIK) "R#4"
                    COMM.PIK = ("0000":COMM.PIK) "R#4"
                    WRITE I2BUFF ON I2.PRODUCT,PS.NUM
               END
          END
          IF NOT(MOD(CT,100)) THEN PRINT CT
     REPEAT

     SELECT COMM
     CT=0
     LOOP
          CT+=1
     WHILE READNEXT COMM.KEY DO
          COMM.KEY.O = COMM.KEY
          READ CBUFF FROM COMM,COMM.KEY THEN
               CBUFF.O = CBUFF
               IF LEN(COMM.KEY) <> 4 THEN
                    DELETE COMM,COMM.KEY
                    COMM.KEY = ("0000":COMM.KEY) "R#4"
                    CBUFF<8> = COMM.KEY
               END
               IF LEN(CBUFF<2>) <> 4 THEN
                    CBUFF<2> = ("000":CBUFF<2>) "R#4"
               END
          END
          IF (COMM.KEY.O <> COMM.KEY OR CBUFF.O <> CBUFF) THEN WRITE CBUFF ON COMM,COMM.KEY
          IF NOT(MOD(CT,100)) THEN PRINT CT
     REPEAT

