     OPEN 'CSV' TO F.CSV ELSE PRINT 'CSV';STOP
     OPEN 'RULES2' TO F.RULES ELSE PRINT 'RULES';STOP
     READ RULES2 FROM  F.CSV,'RULES2.csv' ELSE RULES2 = ''
     RULE.CT = DCOUNT(RULES2,@FM)
     FOR RULE.X = 1 TO RULE.CT
          RBUFF = CHANGE(RULES2<RULE.X>,",",@FM)
          RBUFF = CHANGE(RBUFF,';',@VM)
          RID = RULE.X + 2000
          WRITE RBUFF ON F.RULES,RID
     NEXT RULE.X

