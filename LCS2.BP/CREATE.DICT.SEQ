      PARMS1 = TRIM(FIELD(@SENTENCE," ",2))
      PARMS2 = TRIM(FIELD(@SENTENCE," ",3))
      FILE.NAME = PARMS1
      ITEM.NAME = PARMS2
      DICT.NAME = FIELD(ITEM.NAME,".",1)

      OPENSEQ FILE.NAME,ITEM.NAME TO SFILE ELSE PRINT "SFILE";STOP
      READSEQ LINE FROM SFILE THEN
         CALL CREATE.DICT.SUB(DICT.NAME,LINE)
      END ELSE
         PRINT "HEADER LINE NOT FOUND IN":FILE.NAME:" ":ITEM.NAME
      END
      END


