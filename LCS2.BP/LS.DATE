     FUNCTION LS.DATE(DATE.TEXT)
     IF NUM(DATE.TEXT) THEN I.DATE=DATE.TEXT ELSE I.DATE = ICONV(DATE.TEXT,"D")
     N.DATE = OCONV(I.DATE,"D4-")
     YYYY = FIELD(N.DATE,"-",3)
     MM = "00":FIELD(N.DATE,"-",1) 
     MM = MM "R#2"
     DD = "00":FIELD(N.DATE,"-",2) 
               DD = DD "R#2"
     LS.DATE = YYYY:"-":MM:"-":DD
     RETURN (LS.DATE)
