      OPEN 'INV' TO INV ELSE PRINT 'INV';STOP
      OPEN 'TB' TO TB ELSE PRINT 'TB';STOP
      INV.INSIDE.COST.ATT = 44
      INV.UPC.ATT = 28

      TB.UOM.ATT = 14
      TB.INSIDE.COST.ATT =15

      SELECT INV

      CT=0
      LOOP
         PRINT CT
         CT=CT+1
         DEBUG
         READNEXT INV.ID ELSE EXIT
         PRINT INV.ID
         READ INV.BUFF FROM INV,INV.ID THEN
            UPC = INV.BUFF<INV.UPC.ATT>
            PRINT UPC
            READ TB.BUFF FROM TB,UPC THEN
               TB.UOM = TB.BUFF<TB.UOM.ATT>
               TB.COST.EACH = CHANGE(TB.BUFF<TB.INSIDE.COST.ATT>,"$","")
               PRINT TB.COST.EACH
               IF TB.UOM = "C" THEN TB.COST.EACH /= 100
               IF TB.UOM = "M" THEN TB.COST.EACH /= 1000
               INV.BUFF<INV.INSIDE.COST.ATT> = TB.COST.EACH
               PRINT "WRITING ":INV.BUFF
               WRITE INV.BUFF ON INV,INV.ID
            END
         END
      REPEAT

      END
