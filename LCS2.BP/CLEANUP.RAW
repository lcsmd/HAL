      DEFFUN CLEANUP(AA)
      OPEN 'RAW.EXH.FILES' TO F.RAW.EXH.FILES ELSE PRINT 'RAW.EXH.FILES';STOP
      SELECT F.RAW.EXH.FILES
      EOF = 0
      LOOP
      WHILE NOT(EOF) DO
         READNEXT AA THEN
         PRINT "*":AA
            READ A.B FROM F.RAW.EXH.FILES,AA THEN
               AA2 = CLEANUP(AA)
               PRINT "    ":AA2
               PRINT
               AA2 = 
               WRITE A.B ON F.RAW.EXH.FILES,AA2
            END
         END ELSE EOF = 1
      REPEAT
      END

