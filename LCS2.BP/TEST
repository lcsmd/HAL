     OPEN "I2.PRODUCT" TO I2.PRODUCT ELSE PRINT "I2.PRODUCT";STOP
     EXECUTE "SELECT I2.PRODUCT WITH UPC"
     LOOP
     WHILE READNEXT I2.PIK DO
          READ LIB.B FROM I2.PRODUCT,I2.PIK THEN
               PRINT I2.PIK:"   ":LIB.B<1>:"     ":LIB.B<2>
          END
     REPEAT
