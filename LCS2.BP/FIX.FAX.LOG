     PROGRAM FIX.FAX.LOG
     OPEN "FAX.LOG" TO FAX.LOG ELSE PRINT 'FAX.LOG';STOP
     SELECT FAX.LOG
     LOOP WHILE READNEXT AA DO
          NAA = UPCASE(AA)
          PRINT AA
          IF AA <> NAA THEN
               READ FAX.B FROM FAX.LOG,AA THEN
                    WRITE FAX.B ON FAX.LOG,NAA
                    DELETE FAX.LOG,AA
                    PRINT "WRITING:  " NAA
                    PRINT "DELETING ":AA
               END
          END
     REPEAT

