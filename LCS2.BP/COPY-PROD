*---> Recovered document: COPY.PROD
     OPEN 'PRODUCTQ' TO PRODUCT ELSE PRINT 'PRODUCT';STOP
     OPEN 'PRODUCT.NAME' TO PRODUCT.NEW ELSE PRINT 'PRODUCT.NEW';STOP
     SELECT PRODUCT.NEW
     CT=0
     LOOP
          CT+=1
          IF NOT(MOD(CT,100)) THEN PRINT CT
     WHILE READNEXT AA DO
          READ BUFF FROM PRODUCT.NEW,AA THEN
               WRITE BUFF ON PRODUCT,AA
          END
     REPEAT

