      FUNCTION CLEANUP(JUNKY)
      JUNK = "/?\!@#$%^&*()+=<>;,:[]."
      DOT = DCOUNT(JUNKY,".")
      EXT=".":FIELD(JUNKY,".",DOT+1)
      FNAME=LEFT(JUNKY,LEN(JUNKY)-LEN(EXT))
      JL = LEN(JUNK)
      CONVERT " " TO "_" IN FNAME
      FOR JX = 1 TO JL
         JUNKY = TRIM(FNAME,JUNK[JX,1],"A")
      NEXT JX
      JUNKY:=FNAME:EXT
      RETURN(JUNKY)
      END
