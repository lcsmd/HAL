     LIST.NAME = FIELD(@SENTENCE," ",2)
     * PIVOT MD DICT I2.PRICE.UPDATE.LOG @ALL 2
     IF FIELD(@SENTENCE," ",3) = "DICT" THEN DICT.STAT = 1 ELSE DICT.STAT = 0
     FILE.NAME = FIELD(@SENTENCE," ",3 + DICT.STAT)
     FILE.KEY = FIELD(@SENTENCE," ",4 + DICT.STAT)
     ATT.NUM = FIELD(@SENTENCE," ",5 + DICT.STAT)
     LIST.ROW = ""
     IF DICT.STAT THEN
          OPEN 'DICT',FILE.NAME TO F.FILE ELSE PRINT 'DICT,':FILE.NAME;STOP
     END ELSE
          OPEN FILE.NAME TO F.FILE ELSE PRINT FILE.NAME;STOP
     END
     READLIST LIST.KEYS FROM LIST.NAME THEN
          KEY.CT = DCOUNT(LIST.KEYS,@FM)
          FOR X = 1 TO KEY.CT
               LIST.KEY = LIST.KEYS<X>
               IF LIST.KEY[1,1] <> '@' THEN
                    PRINT LIST.KEY
                    LIST.ROW := LIST.KEY
                    IF X < KEY.CT THEN LIST.ROW := " "
               END
          NEXT X
     END
     IF DICT.STAT AND ATT.NUM = 2 THEN
          WRITEV 'PH' ON F.FILE,FILE.KEY,1
     END
     WRITEV LIST.ROW ON F.FILE,FILE.KEY,ATT.NUM
     PRINT LIST.ROW
     DEBUG
     STOP
