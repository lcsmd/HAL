     PROGRAM CREATE.TRADE

     AFILE.DICT.KEY = "I2"
     AFILE.NAME = 'I2.PRODUCT'
     AFILE.SORT = 'PRICE_SRC_NUMBER'
     AFILE.DESC.ATT = 68
     AFILE.SORT.ATT = 42
     BFILE.NAME = "TRADE"
     BFILE.PATH = 'E:\DATA_ACCOUNTS\PRICE\TRADESERVICE\ARCHIVE\TRADE.TXT'

     INARGS = ""
     INARGS<1> = AFILE.DICT.KEY
     INARGS<2> = AFILE.SORT.ATT
     INARGS<3> = AFILE.NAME
     INARGS<4> = AFILE.SORT
     INARGS<5> = AFILE.DESC.ATT
     INARGS<6> = BFILE.NAME
     INARGS<7> = BFILE.PATH
     
     CALL CREATE.BFILE(INARGS,OUTARGS)
     
     END

