     OPEN 'PRODUCT' TO PRODUCT ELSE PRINT 'PRODUCT';STOP
     OPEN 'PROD.BACK' TO PROD.BACK ELSE PRINT 'PROD.BACK';STOP
     SELECT PROD.BACK
     CT=0
     LOOP
          CT+=1
     WHILE READNEXT AA DO
          READ BBUFF FROM PROD.BACK,AA THEN
               WRITE BBUFF ON PRODUCT,AA
          END
          IF NOT(MOD(CT,100)) THEN PRINT CT
     REPEAT

