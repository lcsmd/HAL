      OPEN 'INVENT' TO INVENT ELSE PRINT 'INVENT';STOP
      SELECT INVENT
      LOOP
      WHILE READNEXT AA DO
         PRINT AA
         NBUFF = ""
         READ BUFF FROM INVENT,AA THEN
            FOR X = 1 TO 11
               NBUFF<X> = BUFF<X>
            NEXT X
            WRITE NBUFF ON INVENT,AA
         END
      REPEAT
      END
